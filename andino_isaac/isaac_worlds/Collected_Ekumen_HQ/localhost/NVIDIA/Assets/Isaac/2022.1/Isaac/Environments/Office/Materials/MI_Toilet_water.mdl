mdl 1.3;

import ::math::*;
import ::state::*;
import ::tex::*;
import ::anno::*;
using OmniUe4Function import *;
using OmniUe4Translucent import *;

export annotation sampler_color();
export annotation sampler_normal();
export annotation sampler_grayscale();
export annotation sampler_alpha();
export annotation sampler_masks();
export annotation sampler_distancefield();
export annotation dither_masked_off();
export annotation world_space_normal();

export material MI_Toilet_water(
	float4 Color_RGB_Opacity_A_ = float4(1.0,1.0,1.0,1.0),
	float Roughness = 0.1)
 = 
	let {
	float3 WorldPositionOffset_mdl = float3(0.0,0.0,0.0);


	float3 Normal_mdl = float3(0.0,0.0,1.0);

	float3 EmissiveColor_mdl = float3(0.0,0.0,0.0);
	float Opacity_mdl = Color_RGB_Opacity_A_.w;
	float OpacityMask_mdl = (math::saturate(Color_RGB_Opacity_A_.w) - 1.0f / 255.0f) < 0.0f ? 0.0f : 1.0f;
	float3 BaseColor_mdl = float3(Color_RGB_Opacity_A_.x,Color_RGB_Opacity_A_.y,Color_RGB_Opacity_A_.z);
	float Metallic_mdl = 0.0;
	float Specular_mdl = 1.0;
	float Roughness_mdl = Roughness;
	float2 Refraction_mdl = float2(float2(1.0,0.0).x,float2(1.0,0.0).x);

	} in
		::OmniUe4Translucent(
			base_color: BaseColor_mdl,
			metallic: Metallic_mdl,
			roughness: Roughness_mdl,
			specular: Specular_mdl,
			normal: Normal_mdl,
			opacity: Opacity_mdl,
			opacity_mask: OpacityMask_mdl,
			emissive_color: EmissiveColor_mdl,
			displacement: WorldPositionOffset_mdl,
			refraction: Refraction_mdl.x,
			two_sided: false);
